================/// extensive, well documented snippet from http://msdn.microsoft.com/en-us/library/ms532998.aspx	function fnInit(tablename,list,column1,list2){//	alert(tablename);//	alert(typeof(list));		var test=list;	//	alert(list.length);//   Declare variables and create the header, footer, and caption.  	var oTable = document.createElement("TABLE");  	var oTHead = document.createElement("THEAD");  	var oTBody0 = document.createElement("TBODY");  	var oCaption = document.createElement("CAPTION");  	var oRow, oCell;	var i;// Declare data that would normally be read in from a stock Web site.  	var heading = new Array();  	heading[0] = column1;// Insert the created elements into oTable.  oTable.appendChild(oTHead);  oTable.appendChild(oTBody0);  oTable.appendChild(oCaption);  // Set the table's border width and colors.// oTable.border=0;// oTable.bgColor="lightslategray";  // Insert a row into the header and set its background color.  oRow = document.createElement("TR");  oTHead.appendChild(oRow);  // Create and insert cells into the header row.if (tablename=="sspTableContainer"){  for (i=0; i<heading.length; i++)  {    oCell = document.createElement("TH");    oCell.innerHTML = heading[i];    oRow.appendChild(oCell);  }  }// hier kun je bijvoorbeeld differentieren voor de ssp en songtype tabellen  // Insert rows and cells into bodies.if (tablename=="sspTableContainer"){ //alert("building ssp table");  for (i=0; i<list.length; i++)  {	var oBody = oTBody0;//	  alert(i);    oRow = document.createElement("TR");    oBody.appendChild(oRow);	for (var j=0; j<2 ; j++)	{ 	if (j==1){      oCell = document.createElement("TD");		oCell.innerHTML = list[i];      oRow.appendChild(oCell);      }    else if (j==0){      oCell = document.createElement("TD");// Z points to empty marker. we figure Z would never be used	oCell.innerHTML = createNumberedIcon_for_table(iconcolours[i],"");      oRow.appendChild(oCell);      }      }    } sspTableContainer.appendChild(oTable);}    else if(tablename=="songTableContainer"){    // alert("building song table");    var oBody = oTBody0;    oRow = document.createElement("TR");    oBody.appendChild(oRow);	oCell = document.createElement("TD");  	var firstcell = "<b>Songtypes</b>";  	oCell.innerHTML = firstcell;  	oRow.appendChild(oCell);    	for (i=0; i<list.length; i++)  	{		for (j=0; j<2 ; j++)		{ 		if (j==0){      	oCell = document.createElement("TD");      	var iconnumber = list2[i];	  	oCell.innerHTML = createNumberedIcon_for_table(iconcolours[13],iconcodes[iconnumber]);      	oRow.appendChild(oCell);      	}    	if (j==1){      	oCell = document.createElement("TD");	  	oCell.innerHTML = list[i];      	oRow.appendChild(oCell);      }      }  }     songTableContainer.appendChild(oTable);  }  }               var request = GXmlHttp.create();        request.open("GET", "dataJQBNYKBKDW.xml", true);        request.onreadystatechange = function() {        if (request.readyState == 4) {          var xmlDoc = request.responseXML;          var markers = xmlDoc.documentElement.getElementsByTagName("marker");// var ssp_list = array to be filled with different ssp's'//initialise ssp_count at zero.          var ssp_list = new Array('not mentioned');          var ssp_count = 0;          var ssp_nr = 0;          var songtype_list = new Array('call','song','song and call','alarm','other','not mentioned');          var songtypenr_list = new Array(0,1,2,3,4,25);          var songtype_nr = 0;          var songtype_count = 0;                    for (var i = 0; i < markers.length; i++) {            var point = new GLatLng(parseFloat(markers[i].getAttribute("lat")),                             parseFloat(markers[i].getAttribute("lng")));                                         // ==== Each time a point is found, extend the bounds to include it =====            bounds.extend(point);            var text = markers[i].getAttribute("loc");            var url = markers[i].getAttribute("url");            var plaatje = markers[i].getAttribute("fft");             var recordist = markers[i].getAttribute("recordist");             var cat = markers[i].getAttribute("sndnr");			var catbg = markers[i].getAttribute("sndnrbg");            var mp3path = markers[i].getAttribute("mp3");	    	var tab = markers[i].getAttribute("tab");            var geluid = "http://www.xeno-canto.org/" + mp3path + "";            var fg = markers[i].getAttribute("fg");            //=====================================================//// subspecies & songtype handling////=====================================================              var songtype = markers[i].getAttribute("songtype");                   	// first deal with songtypes        	// check songtype, kan helemaal onafhankelijk van ssp        	// is songtype gegeven?        	// a songtype_nr is always assigned (for the markers in the map)        	//         	// only if a new songtype is found, it is added to the list, and at the same time a number is defined 			// and added to the number list        	        	if (emptysongtype.indexOf(songtype) == -1){   			songtype=songtype.toLowerCase(); 			// only to lower case if a songtype is mentioned. argument has to be string  								if(songtype.indexOf("alarm")!= -1){					songtype_nr=3;					songtypenr_list.push(songtype_nr);//					songtype_list.push("alarm");					songtype="alarm";//					alert("songtype filed as alarm");					}          									else if(songtype.indexOf("call")!= -1){					 	if(songtype.indexOf("song")!= -1){						songtype_nr=2;						songtypenr_list.push(songtype_nr);						songtype="song and call";						}					else{					songtype_nr=0;					songtypenr_list.push(songtype_nr);					songtype="call";//					songtype_list.push("call");//					alert("songtype filed as call");					}					}          					else if(songtype.indexOf("song")!= -1){					songtype_nr=1;					songtypenr_list.push(songtype_nr);					songtype="song";//					songtype_list.push("song");//					alert("songtype filed as song");					}					else{					songtype_nr= 4;//					songtype="other";//					songtypenr_list.push(songtype_nr);//					alert("songtype not predefined");					}            	}//end for songtype is given			else{			 // this is part of the emptylist and songtype_nr is always 25				songtype="\<i\>not mentioned\</i\>";				songtype_nr=25;//            	songtype_list.push(songtype); 				songtypenr_list.push(songtype_nr);// add to list			 }						// now deal with subspecies            var ssp = markers[i].getAttribute("ssp");             //is ssp gegeven? compare to emptyssp		  						          	if (emptyssp.indexOf(ssp) == -1){ 			   	             // if ssp not an element of emptyssp then emptyssp.indexOf(ssp)== -1 therefore a ssp is mentioned and we can add it to ssp_list                     	if (ssp_list.indexOf(ssp) == -1){            		ssp_count+=1; // smallest number =1, 0 reserved for ssp not mentioned            		ssp_nr=ssp_count; // ssp_nr used in choosing the icon....            		ssp_list.push(ssp); // add to list					}									// if it is already in the list take the index as ssp_count				// so we have to find the index of the element of ssp_list that matches ssp							else {					 	ssp_nr=ssp_list.indexOf(ssp);			 	}			}			// else ssp is not mentioned, and we assign this to not mentioned and to number 0			else{	            		ssp_nr=0; // ssp_nr used in choosing the icon....					}											///================================================================================================									    	// start a new set of arrays if we find a new set of tabs            			if (tab == 0) {	      	// create the previous marker	      		var tabs_html = [];	      		var tabs_labels = [];	    	}                        if (fg == 1) {			 //foreground species, check for ssp etc.             // var icon_current moet dus eigenlijk wisselen naar 			 // gelang de knop die wordt ingedrukt.....             // makkelijkste is iconssp die moet dan wel ergens 			 // worden gedefinieerd, kiezen uit een standaardrij         	 //   