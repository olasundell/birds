     }              else if ((map.getZoom() > 4) && (map.getZoom() < 8)){          this.myLayers = "srtm3_2";       }              else if (map.getZoom() > 8) {          this.myLayers = "srtm3_3";       }        lURL+="&REQUEST=GetMap";       lURL+="&SERVICE=WMS";       lURL+="&VERSION=1.1.1";       lURL+="&LAYERS="+this.myLayers;       lURL+="&STYLES="+this.myStyles;       lURL+="&FORMAT="+this.myFormat;       lURL+="&BGCOLOR=0xFFFFFF";       lURL+="&TRANSPARENT=TRUE";       lURL+="&SRS="+lSRS;       lURL+="&BBOX="+lBbox;       lURL+="&WIDTH=256";       lURL+="&HEIGHT=256";       lURL+="&reaspect=false";       return lURL;     }      var map = new GMap2(document.getElementById("map"));          // Create tile layers      var tileICEDS1= new GTileLayer(new GCopyrightCollection("col1"),3,17);                tileICEDS1.myFormat="image/jpeg";                tileICEDS1.myStyles="default";                tileICEDS1.myBaseURL="http://iceds.ge.ucl.ac.uk/cgi-bin/icedswms?";                tileICEDS1.getTileUrl=CustomGetTileUrlICEDS;      //define layers, hybrid maps consist of more than 1 layer          var layer0=[G_NORMAL_MAP.getTileLayers()[0]];          var layer1=[G_HYBRID_MAP.getTileLayers()[0],G_HYBRID_MAP.getTileLayers()[1]];       var layer2=[tileICEDS1];      var layer4=[G_PHYSICAL_MAP.getTileLayers()[0],G_HYBRID_MAP.getTileLayers()[1]];      //define maps  //  var custommap0 = new GMapType(layer0, G_SATELLITE_MAP.getProjection(), "Map");      var custommap1 = new GMapType(layer1, G_SATELLITE_MAP.getProjection(), "Sat");          var custommap2 = new GMapType(layer2, G_SATELLITE_MAP.getProjection(), "Alt");              var custommap4 = new GMapType(layer4, G_SATELLITE_MAP.getProjection(), "Terrain");      map.getMapTypes().length = 0;      //---- hier worden de maps neergezet-------      map.addMapType(custommap4);   // map.addMapType(custommap0);      map.addMapType(custommap1);      map.addMapType(custommap2);      map.addMapType(G_SATELLITE_3D_MAP);      map.a