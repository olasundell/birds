= markers[i].getAttribute("fft");             var recordist = markers[i].getAttribute("recordist");             var cat = markers[i].getAttribute("sndnr");			var catbg = markers[i].getAttribute("sndnrbg");            var mp3path = markers[i].getAttribute("mp3");	    	var tab = markers[i].getAttribute("tab");            var geluid = "http://www.xeno-canto.org/" + mp3path + "";            var fg = markers[i].getAttribute("fg");            //=====================================================//// subspecies & songtype handling////=====================================================              var songtype = markers[i].getAttribute("songtype");                   	// first deal with songtypes        	// check songtype, kan helemaal onafhankelijk van ssp        	// is songtype gegeven?        	// a songtype_nr is always assigned (for the markers in the map)        	//         	// only if a new songtype is found, it is added to the list, and at the same time a number is defined 			// and added to the number list        	        	if (emptysongtype.indexOf(songtype) == -1){   			songtype=songtype.toLowerCase(); 			// only to lower case if a songtype is mentioned. argument has to be string  								if(songtype.indexOf("alarm")!= -1){					songtype_nr=3;					songtypenr_list.push(songtype_nr);//					songtype_list.push("alarm");					songtype="alarm";//					alert("songtype filed as alarm");					}          									else if(songtype.indexOf("call")!= -1){					 	if(songtype.indexOf("song")!= -1){						songtype_nr=2;						songtypenr_list.push(songtype_nr);						songtype="song and call";						}					else{					songtype_nr=0;					songtypenr_list.push(songtype_nr);					songtype="call";//					songtype_list.push("call");//					alert("songtype filed as call");					}					}          					else if(songtype.indexOf("song")!= -1){					songtype_nr=1;					songtypenr_list.push(songtype_nr);					songtype="song";//					songtype_list.push("song");//					alert("songtype filed as song");					}					else{					songtype_nr= 4;//					songtype="other";//					songtypenr_list.push(songtype_nr);//					alert("songtype not predefined");					}            	}//end for songtype is given			else{			 // this is part of the emptylist and songtype_nr is always 25				songtype="\<i\>not mentioned\</i\>";				songtype_nr=25;//            	songtype_list.push(songtype); 				songtypenr_list.push(songtype_nr);// add to list			 }						// now deal with subspecies            var ssp = markers[i].getAttribute("ssp");             //is ssp gegeven? compare to emptyssp		  						          	if (emptyssp.indexOf(ssp) == -1){ 			   	             // if ssp not an element of emptyssp then emptyssp.indexOf(ssp)== -1 therefore a ssp is mentioned and we can add it to ssp_list                     	if (ssp_list.indexOf(ssp) == -1){            		ssp_count+=1; // smallest number =1, 0 reserved for ssp not mentioned            		ssp_nr=ssp_count; // ssp_nr used in choosing the icon....            		ssp_list.push(ssp); // add to list					}									// if it is already in the list take the index as ssp_count				// so we have to find the index of the element of ssp_list that matches ssp							else {					 	ssp_nr=ssp_list.indexOf(ssp);			 	}			}			// else ssp is not mentioned, and we assign this to not mentioned and to number 0			else{	            		ssp_nr=0; // ssp_nr used in choosing the icon....					}											///================================================================================================									    	// start a new set of arrays if we find a new set of tabs            			if (tab == 0) {	      	// create the previous marker	      		var tabs_html = [];	      		var tabs_labels = [];	    	}                        if (fg == 1) {			 //foreground species, check for ssp etc.             // var icon_current moet dus eigenlijk wisselen naar 			 // gelang de knop die wordt ingedrukt.....             // makkelijkste is iconssp die moet dan wel ergens 			 // worden gedefinieerd, kiezen uit een standaardrij         	 //    if (ssp_nr==0){			 //		var icon_current=icon;			 //	}			 //	else{			 //reduce ssp_nr with 1 to start of with red			 // ssp_nr-=1;              var icon_current = createNumberedIcon(iconcolours[ssp_nr],iconcodes[songtype_nr]);        	 //     } var html = "<div class='small_font'><h6>" + ssp +"  "+ songtype +"</h6><br><img height='100' src='" + plaatje + "'><br><object classid='clsid:D27CDB6E-AE6D-11cf-96B8-444553540000' codebase='http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,47,0' width='15' height='15' id='wimpybutton465'> <param name='movie' value='http://www.xeno-canto.org/sounds/wimpy_button.swf' /> <param name='loop' value='false' /><param name='menu' value='false' /><param name='quality' value='high' /><param name='bgcolor' value='#F9F9A3' /><param name='wmode' value='transparent' /><param name='flashvars' value='theFile=%2B%20" + geluid + "&bufferAudio=10&wimpyReg=N3RONXRrJTVEJTgxTSU4MFByUjBVNCUyRnpJJTdGcS1mOHg1azN0UGUlMkN4&playingColor=F9F9A3&grinderColor=BF3C12&rollOverColor=BF3C12' /><embed src='http://www.xeno-canto.org/sounds/wimpy_button.swf' flashvars='theFile=%2B%20" + geluid + "&bufferAudio=10&wimpyReg=N3RONXRrJTVEJTgxTSU4MFByUjBVNCUyRnpJJTdGcS1mOHg1azN0UGUlMkN4&playingColor=F9F9A3&grinderColor=BF3C12&rollOverColor=BF3C12' width='15' height='15' bgcolor='#F9F9A3' loop='false' menu='false' quality='high' name='wimpybutton465' align='top' allowScriptAccess='sameDomain' type='application/x-shockwave-flash' pluginspage='http://www.macromedia.com/go/getflashplayer' wmode='transparent'/></object><img src = 'white.jpg' border = '0' width = '0' height = '0''> <a href='browse.php?query=nr:" + cat + "'>   XC" + cat + "</a>  "+ recordist +"  <img src = 'white.jpg' border = '0' width = '10' height = '1''><a href='http://creativecommons.org/licenses/by-nc-nd/3.0/'><img style='border-width:0' src='http://www.xeno-canto.org/cc.logo.circlesmallest.png'></a></div>";            }            if (fg == 0) {// background species            	html = "<div class='small_font'><a href='browse.php?query=loc:" + url + "'>" 		+ text + "</a><br>In background of <a href='browse.php?query=nr:" + catbg + "'>   XC" + catbg + "</a><br><img src='" + plaatje + "'><br></div>";	      		var icon_current = icon_green;             }                      if (tab == 0) {  	      		var marker = createTabbedMarker(point, icon_current, tabs_html, tabs_labels);              	map.addOverlay(marker);            }            tabs_labels.push(tab);	    	tabs_html.push(html);          }// loop over all markers 		                map.setZoom(map.getBoundsZoomLevel(bounds));        // run fnInit to make table with subspecies and markers        fnInit("sspTableContainer",ssp_list,"Subspecies",ssp_list);        //run fnInit to make table with call-types and letters        fnInit("songTableContainer",songtype_list,"Songtype",songtypenr_list);        }//getreadystate==4              }//request.onreadystatechange      request.send(null);      }//GMapLoad//window.onload=fnInit;</script>        <script type="text/javascript" src="http://www.xeno-canto.org/sounds/wimpy_button.js"></script>	</head>	<body onload="init(); GMapLoad();" onunload="GUnload();">        <script type="text/javascript" src="http://www.xeno-canto.org/ajax.js"></script>        <script type="text/javascript" src="http://www.xeno-canto.org/ajax-dynamic-list.js"></script><style type="text/css">                /* Big box with list of options */        #ajax_listOfOptions{                position:absolute;      /* Never change this one */                width:405px;    /* Width of box */                height:250px;   /* Height of box */                overflow:auto;  /* Scrolling features */                border:1px solid #8D2B12;       /* Dark green border */                background-color:#FFF;  /* White background color */                text-align:left;                font-size:0.9em;                z-index:100;        }        #ajax_listOfOptions 