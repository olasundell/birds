ound species            	html = "<div class='small_font'><a href='browse.php?query=loc:" + url + "'>" 		+ text + "</a><br>In background of <a href='browse.php?query=nr:" + catbg + "'>   XC" + catbg + "</a><br><img src='" + plaatje + "'><br></div>";	      		var icon_current = icon_green;             }                      if (tab == 0) {  	      		var marker = createTabbedMarker(point, icon_current, tabs_html, tabs_labels);              	map.addOverlay(marker);            }            tabs_labels.push(tab);	    	tabs_html.push(html);          }// loop over all markers 		                map.setZoom(map.getBoundsZoomLevel(bounds));        // run fnInit to make table with subspecies and markers        fnInit("sspTableContainer",ssp_list,"Subspecies",ssp_list);        //run fnInit to make table with call-types and letters        fnInit("songTableContainer",songtype_list,"Songtype",songtypenr_list);        }//getreadystate==4              }//request.onreadystatechange      request.send(null);      }//GMapLoad//window.onload=fnInit;</script>        <script type="text/javascript" src="http://www.xeno-canto.org/sounds/wimpy_button.js"></script>	</head>	<body onload="init(); GMapLoad();" onunload="GUnload();">        <script type="text/javascript" src="http://www.xeno-canto.org/ajax.js"></script>        <script type="text/javascript" src="http://www.xeno-canto.org/ajax-dynamic-list.js"></script><style type="text/css">                /* Big box with list of options */        #ajax_listOfOptions{                position:absolute;      /* Never change this one */                width:405px;    /* Width of box */                height:250px;   /* Height of box */                overflow:auto;  /* Scrolling features */                border:1px solid #8D2B12;       /* Dark green border */                background-color:#FFF;  /* White background color */                text-align:left;                font-size:0.9em;                z-index:100;        }        #ajax_listOfOptions