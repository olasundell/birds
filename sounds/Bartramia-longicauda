redicon.iconSize = new GSize(14, 22);      	numberedicon.shadowSize = new GSize(24, 22);      	numberedicon.iconAnchor = new GPoint(6, 20);      	numberedicon.infoWindowAnchor = new GPoint(5, 1);        return numberedicon;      }            function createNumberedIcon_for_table(colour,code){       	var numberedicon_for_table = "<img src='/markers/" + colour+ "_Marker" + code + ".png' height='21' width='12'>";    	return numberedicon_for_table;      }              var icon = new GIcon();      	icon.image = "/markers/Red_Marker.png";      	icon.shadow = "/markers/mm_20_shadow.png";      	icon.iconSize = new GSize(12,20);      	icon.shadowSize = new GSize(22,20);      	icon.iconAnchor = new GPoint(6,20);      	icon.infoWindowAnchor = new GPoint(5,1);            var icon_green = new GIcon();      	icon_green.image = "/markers/LimeGreen_10backgroundMarker.png";      	icon_green.shadow = "/markers/mm_20_shadow.png";      	icon_green.iconSize = new GSize(8,10);      	icon_green.shadowSize = new GSize(8,8);      	icon_green.iconAnchor = new GPoint(5,5);      	icon_green.infoWindowAnchor = new GPoint(1,1);            function createTabbedMarker(point,icon,htmls,labels) {       var marker = new GMarker(point,icon);        	GEvent.addListener(marker, "click", function() {        	// adjust the width so that the info window is large enough for this many tabs          if (htmls.length > 2) {            htmls[0] = '<div style="width:'+htmls.length*88+'px">' + htmls[0] + '</div>';          }          var tabs = [];          for (var i=0; i<htmls.length; i++) {            tabs.push(new GInfoWindowTab(labels[i],htmls[i]));          }          marker.openInfoWindowTabsHtml(tabs);        });        return marker;      }// always include infonatura range using kml file generated for this species..         var gx = new GGeoXml("http://www.xeno-canto.org/ranges/rangeufmiws.kml");        //map.addOverlay(gx);                ///=============================================================