dicon.iconSize = new GSize(14, 22);      	numberedicon.shadowSize = new GSize(24, 22);      	numberedicon.iconAnchor = new GPoint(6, 20);      	numberedicon.infoWindowAnchor = new GPoint(5, 1);        return numberedicon;      }            function createNumberedIcon_for_table(colour,code){       	var numberedicon_for_table = "<img src='/markers/" + colour+ "_Marker" + code + ".png' height='21' width='12'>";    	return numberedicon_for_table;      }              var icon = new GIcon();      	icon.image = "/markers/Red_Marker.png";      	icon.shadow = "/markers/mm_20_shadow.png";      	icon.iconSize = new GSize(12,20);      	icon.shadowSize = new GSize(22,20);      	icon.iconAnchor = new GPoint(6,20);      	icon.infoWindowAnchor = new GPoint(5,1);            var icon_green = new GIcon();      	icon_green.image = "/markers/LimeGreen_10backgroundMarker.png";      	icon_green.shadow = "/markers/mm_20_shadow.png";      	icon_green.iconSize = new GSize(8,10);      	icon_green.shadowSize = new GSize(8,8);      	icon_green.iconAnchor = new GPoint(5,5);      	icon_green.infoWindowAnchor = new GPoint(1,1);            function createTabbedMarker(point,icon,htmls,labels) {       var marker = new GMarker(point,icon);        	GEvent.addListener(marker, "click", function() {        	// adjust the width so that the info window is large enough for this many tabs          if (htmls.length > 2) {            htmls[0] = '<div style="width:'+htmls.length*88+'px">' + htmls[0] + '</div>';          }          var tabs = [];          for (var i=0; i<htmls.length; i++) {            tabs.push(new GInfoWindowTab(labels[i],htmls[i]));          }          marker.openInfoWindowTabsHtml(tabs);        });        return marker;      }// always include infonatura range using kml file generated for this species..         var gx = new GGeoXml("http://www.xeno-canto.org/ranges/rangezzhgne.kml");        //map.addOverlay(gx);                ///=================================================================================================/// extensive, well documented snippet from http://msdn.microsoft.com/en-us/library/ms532998.aspx	function fnInit(tablename,list,column1,list2){//	alert(tablename);//	alert(typeof(list));		var test=list;	//	alert(list.length);//   Declare variables and create the header, footer, and caption.  	var oTable = document.createElement("TABLE");  	var oTHead = document.createElement("THEAD");  	var oTBody0 = document.createElement("TBODY");  	var oCaption = document.createElement("CAPTION");  	var oRow, oCell;	var i;// Declare data that would normally be read in from a stock Web site.  	var heading = new Array();  	heading[0] = column1;// Insert the created elements into oTable.  oTable.appendChild(oTHead);  oTable.appendChild(oTBody0);  oTable.appendChild(oCaption);  // Set the table's border width and colors.// oTable.border=0;// oTable.bgColor="lightslategray";  // Insert a row into the header and set its background color.  oRow = document.createElement("TR");  oTHead.appendChild(oRow);  // Create and insert cells into the header row.if (tablename=="sspTableContainer"){  for (i=0; i<heading.length; i++)  {    oCell = document.createElement("TH");    oCell.innerHTML = heading[i];    oRow.appendChild(oCell);  }  }// hier kun je bijvoorbeeld differentieren voor de ssp en songtype tabellen  // Insert rows and cells into bodies.if (tablename=="sspTableContainer"){ //alert("building ssp table");  for (i=0; i<list.length; i++)  {	var oBody = oTBody0;//	  alert(i);    oRow = document.createElement("TR");    oBody.appendChild(oRow);	for (var j=0; j<2 ; j++)	{ 	if (j==1){      oCell = document.createElement("TD");		oCell.innerHTML = list[i];      oRow.appendChild(oCell);      }    else if (j==0){      oCell = document.createElement("TD");// Z points to empty marker. we figure Z would never be used	oCell.innerHTML = createNumberedIcon_for_table(iconcolours[i],"");      oRow.appendChild(oCell);      }      }    } sspTableContainer.appendChild(oTabl