======================/// extensive, well documented snippet from http://msdn.microsoft.com/en-us/library/ms532998.aspx	function fnInit(tablename,list,column1,list2){//	alert(tablename);//	alert(typeof(list));		var test=list;	//	alert(list.length);//   Declare variables and create the header, footer, and caption.  	var oTable = document.createElement("TABLE");  	var oTHead = document.createElement("THEAD");  	var oTBody0 = document.createElement("TBODY");  	var oCaption = document.createElement("CAPTION");  	var oRow, oCell;	var i;// Declare data that would normally be read in from a stock Web site.  	var heading = new Array();  	heading[0] = column1;// Insert the created elements into oTable.  oTable.appendChild(oTHead);  oTable.appendChild(oTBody0);  oTable.appendChild(oCaption);  // Set the table's border width and colors.// oTable.border=0;// oTable.bgColor="lightslategray";  // Insert a row into the header and set its background color.  oRow = document.createElement("TR");  oTHead.appendChild(oRow);  // Create and insert cells into the header row.if (tablename=="sspTableContainer"){  for (i=0; i<heading.length; i++)  {    oCell = document.createElement("TH");    oCell.innerHTML = heading[i];    oRow.appendChild(oCell);  }  }// hier kun je bijvoorbeeld differentieren voor de ssp en songtype tabellen  // Insert rows and cells into bodies.if (tablename=="sspTableContainer"){ //alert("building ssp table");  for (i=0; i<list.length; i++)  {	var oBody = oTBody0;//	  alert(i);    oRow = document.createElement("TR");    oBody.appendChild(oRow);	for (var j=0; j<2 ; j++)	{ 	if (j==1){      oCell = document.createElement("TD");		oCell.innerHTML = list[i];      oRow.appendChild(oCell);      }    else if (j==0){      oCell = document.createElement("TD");// Z points to empty marker. we figure Z would never be used	oCell.innerHTML = createNumberedIcon_for_table(iconcolours[i],"");      oRow.appendChild(oCell);      }      }    } sspTableContainer.appendChild(oTable);}    else